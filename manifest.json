{
   "name":"Trunk",
   "version":"0.1.0",
   "description":"Trunk Capstone Project",
   "permissions":[
      "storage",
      "declarativeContent",
      "background",
      "clipboardWrite",
      "clipboardRead"
   ],
   "background":{
      "scripts":[
         "background.js"
      ],
      "persistent":false
   },
    "browser_action":{
      "default_popup":"popup.html"
   },
    "options_page": "/options/options.html",
   "manifest_version":2
}