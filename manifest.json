{
   "name":"Trunk",
   "version":"0.1.0",
   "description":"Trunk Capstone Project",
   "permissions":[
      "storage",
      "declarativeContent",
      "background",
      "clipboardWrite",
      "clipboardRead"
   ],
   "background":{
      "scripts":[
         "background.js"
      ],
      "persistent":false
   },
    "browser_action":{
      "default_popup":"popup.html"
   },
    "options_page": "/options/options.html",
   "manifest_version":2,
   "content_security_policy": "script-src-elem https://*.bootstrapcdn.com https://*.jsdelivr.net https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js; "
}

